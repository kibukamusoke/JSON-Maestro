{
  "name": "json-maestro",
  "displayName": "JSON Maestro",
  "description": "A beautiful JSON browser and editor with modern UI for easy visualization and editing. If you find it useful, consider supporting development: https://www.buymeacoffee.com/trevorsuna",

  "version": "0.0.23",
  "icon": "icon.png",
  "galleryBanner": {
    "color": "#2C3E50",
    "theme": "dark"
  },
  "keywords": [
    "json",
    "viewer",
    "editor",
    "browser",
    "formatter",
    "tree",
    "visualization"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kbukamusoke/JSON-Maestro.git"
  },
  "engines": {
    "vscode": "^1.96.0"
  },
  "publisher": "TrevorSuna",
  "categories": [
    "Other",
    "Formatters",
    "Visualization"
  ],
  "activationEvents": [
    "onCommand:json-maestro.openBrowser"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "json-maestro.openBrowser",
        "title": "Open JSON Maestro"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "resourceLangId == json",
          "command": "json-maestro.openBrowser",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "lint": "eslint src --ext ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run compile && npx vsce package",
    "prepare-icons": "node convert-icon.js"
  },
